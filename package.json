{
  "name": "pidgey-crush",
  "version": "0.3.0",
  "author": "Esther Kool",
  "description": "A tool to help you evolve Pokemon efficiently in the game Pokemon Go",
  "license": "The MIT License",
  "repository": {
    "type": "git",
    "url": "https://github.com/delicatesther/pidgeycrush.git"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "chalk": "^2.3.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-cssfont64": "0.0.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-if": "^1.0.3",
    "gulp-imagemin": "^4.0.0",
    "gulp-merge-media-queries": "^0.2.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylelint": "^5.0.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "gulp.spritesmith": "^6.5.0",
    "merge-stream": "",
    "node-notifier": "^5.1.2",
    "npm": "^5.8.0",
    "run-sequence": "1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.11.0"
  },
  "paths": {
    "sass": {
      "src": "src/sass/",
      "dest": "dest/css/"
    },
    "css": {
      "src": "src/css/",
      "dest": "dest/css/"
    },
    "js": {
      "src": "src/js/",
      "dest": "dest/js/"
    },
    "images": {
      "src": "src/img/",
      "dest": "dest/img/"
    },
    "iconfont": {
      "base": "dest/",
      "conf": "src/conf/",
      "src": "src/img/iconfont/",
      "destScss": "../../src/sass/04-autogenerated/",
      "destFont": "dest/fonts/",
      "destFontRelative": "../fonts/"
    },
    "base64css": {
      "src": "dest/fonts/",
      "destcss": "src/sass/04-autogenerated/"
    },
    "sprites": {
      "src": "src/img/sprites/",
      "src_retina": "src/img/sprites/sprite2x/",
      "dest_css": "src/sass/04-autogenerated/",
      "destRelative": "../img/",
      "dest": "dest/img/"
    }
  }
}
